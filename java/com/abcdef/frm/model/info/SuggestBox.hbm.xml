<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.abcdef.frm.model.info.SuggestBox" table="frm_suggest_box">
		<id name="boxId" type="java.lang.Long">
			<generator class="native"></generator>
		</id>
		<property name="subject" type="java.lang.String" column="subject"
			length="256" not-null="true" />
		<property name="content" type="java.lang.String" column="content"
			length="4000" not-null="true" />
		<property name="createtime" type="java.util.Date" column="createtime" />
		<property name="recUid" type="java.lang.Long" column="recUid" />
		<property name="recFullname" type="java.lang.String" column="recFullname"
			length="32" />
		<property name="senderId" type="java.lang.Long" column="senderId" />
		<property name="senderFullname" type="java.lang.String"
			column="senderFullname" length="32" />
		<property name="senderIp" type="java.lang.String" column="senderIp"
			length="64" />
		<property name="phone" type="java.lang.String" column="phone"
			length="64" />
		<property name="email" type="java.lang.String" column="email"
			length="100" />
		<property name="isOpen" type="java.lang.Short" column="isOpen" />
		<property name="replyContent" type="java.lang.String" column="replyContent"
			length="4000" />
		<property name="replyTime" type="java.util.Date" column="replyTime" />
		<property name="replyId" type="java.lang.Long" column="replyId" />
		<property name="replyFullname" type="java.lang.String"
			column="replyFullname" length="32" />
		<property name="status" type="java.lang.Short" column="status" />
		<property name="queryPwd" type="java.lang.String" column="queryPwd"
			length="128" />
	</class>
</hibernate-mapping>
