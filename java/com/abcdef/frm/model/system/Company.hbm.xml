<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.abcdef.frm.model.system.Company" table="frm_company"> 
    <id name="companyId" type="java.lang.Long"> 
      <generator class="native"></generator> 
    </id>  
    <property name="companyNo" type="java.lang.String" length="40"/>  
    <property name="companyName" type="java.lang.String" not-null="true" length="40"/>  
    <property name="companyDesc" type="java.lang.String" length="200"/>  
    <property name="legalPerson" type="java.lang.String" length="40"/>  
    <property name="setup" type="java.util.Date"/>  
    <property name="phone" type="java.lang.String" length="40"/>  
    <property name="fax" type="java.lang.String" length="40"/>  
    <property name="site" type="java.lang.String" length="100"/>  
    <property name="logo" type="java.lang.String" length="200"/> 
  </class> 
</hibernate-mapping>
