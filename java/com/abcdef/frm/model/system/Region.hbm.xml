<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.abcdef.frm.model.system.Region" table="frm_region">
		<id name="regionId" column="regionId" type="java.lang.Long">
			<generator class="native"></generator>
		</id>
		<property name="regionName" type="java.lang.String" column="regionName"
			length="128" not-null="true" />
		<property name="regionType" type="java.lang.Short" column="regionType"
			not-null="true" />
		<property name="parentId" type="java.lang.Long" column="parentId" />
	</class>
</hibernate-mapping>
