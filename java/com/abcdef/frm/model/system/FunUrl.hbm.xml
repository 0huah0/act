<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.abcdef.frm.model.system.FunUrl" table="frm_fun_url"> 
    <id name="urlId" column="urlId" type="java.lang.Long" length="19"> 
      <generator class="native"></generator> 
    </id>  
    <property name="urlPath" type="java.lang.String" column="urlPath" length="128" not-null="true"/>  
    <many-to-one name="appFunction" class="com.abcdef.frm.model.system.AppFunction"> 
      <column name="functionId"/> 
    </many-to-one> 
  </class> 
</hibernate-mapping>
