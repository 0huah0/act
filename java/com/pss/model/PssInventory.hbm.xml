<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.pss.model">
	<class name="PssInventory" table="pss_inventory">

		<composite-id>
			<key-property name="warehouseId"  column="WAREHOUSE_ID" type="java.lang.String"/>
			<key-property name="materialId" column="MATERIAL_ID" type="java.lang.String"/>
		</composite-id>
		
		<property 
			name="alertNum" 
			column="ALERT_NUM" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="false" 
			unique="false" 
			length="8"/>
		
		<property 
			name="allNum" 
			column="ALL_NUM" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="false" 
			unique="false" 
			length="8"/>
		
		<property 
			name="goodPdtNum" 
			column="GOOD_PDT_NUM" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="false" 
			unique="false" 
			length="8"/>
		
		<property 
			name="rejectsNum" 
			column="REJECTS_NUM" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="false" 
			unique="false" 
			length="8"/>
		
		<property 
			name="createDate" 
			column="CREATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="createBy" 
			column="CREATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
		<property 
			name="updateDate" 
			column="UPDATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateBy" 
			column="UPDATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
    
	</class>
</hibernate-mapping>