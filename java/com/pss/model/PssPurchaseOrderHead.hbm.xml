<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.pss.model">
    <class name="PssPurchaseOrderHead" table="pss_purchase_order_head">
		
		<id name="poHeadId" column="PO_HEAD_ID">
            <generator class="com.pss.util.GeneratePK" >
              <param name="classname">PssPurchaseOrderHead</param>
              <param name="pk">poHeadId</param>
              <param name="sign">PO</param>
              <param name="idLength">13</param>
            </generator>
        </id>
		
		
		<property 
			name="supplierId" 
			column="SUPPLIER_ID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="50"/>
		
		<property 
			name="priceAmount" 
			column="PRICE_AMOUNT" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="false" 
			unique="false" 
			length="15"/>
		
		<property 
			name="salePriceAmount" 
			column="SALE_PRICE_AMOUNT" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="false" 
			unique="false" 
			length="15"/>
		
		<property 
			name="payAmount" 
			column="PAY_AMOUNT" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="false" 
			unique="false" 
			length="15"/>
		
		<property 
			name="remark" 
			column="REMARK" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="1000"/>
		
		<property 
			name="createDate" 
			column="CREATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="createBy" 
			column="CREATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
		<property 
			name="updateDate" 
			column="UPDATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateBy" 
			column="UPDATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

