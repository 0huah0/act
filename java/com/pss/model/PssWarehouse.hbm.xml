<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.pss.model">
    <class name="PssWarehouse" table="pss_warehouse">

		<id name="warehouseId" column="WAREHOUSE_ID">
            <generator class="com.pss.util.GeneratePK" >
              <param name="classname">PssWarehouse</param>
              <param name="pk">warehouseId</param>
              <param name="sign">WAREHOUSE</param>
              <param name="idLength">13</param>
            </generator>
        </id>
		
		<property 
			name="name" 
			column="NAME" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="50"/>
		
		<property 
			name="desc" 
			column="DESC_" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="1000"/>
		
		<property 
			name="createDate" 
			column="CREATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="createBy" 
			column="CREATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
		<property 
			name="updateDate" 
			column="UPDATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateBy" 
			column="UPDATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

