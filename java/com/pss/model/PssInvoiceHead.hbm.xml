<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.pss.model">
    <class name="PssInvoiceHead" table="pss_invoice_head">

		
		<id name="invoiceHeadId" column="INVOICE_HEAD_ID">
            <generator class="com.pss.util.GeneratePK" >
              <param name="classname">PssInvoiceHead</param>
              <param name="pk">invoiceHeadId</param>
              <param name="sign">PI</param>
              <param name="idLength">13</param>
            </generator>
        </id>
		
		<property 
			name="cusOrSupId" 
			column="CUS_OR_SUP_ID" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="50"/>
		
		<property 
			name="invAmount" 
			column="INV_AMOUNT" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="true" 
			unique="false" 
			length="15"/>
		
		<property 
			name="type" 
			column="TYPE" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="true" 
			unique="false" 
			length="10"/>
		
		<property 
			name="createDate" 
			column="CREATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="createBy" 
			column="CREATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
		<property 
			name="updateDate" 
			column="UPDATE_DATE" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="updateBy" 
			column="UPDATE_BY" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="50"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

