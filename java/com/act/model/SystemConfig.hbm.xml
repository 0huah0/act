<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.act.model.SystemConfig" table="system_config">
		<id name="id" type="java.lang.Long" column="config_id">
			<generator class="native"/>
		</id>
		
		<property name="configType" column="config_type" type="java.lang.String" length="100" not-null="true"/>
		<property name="configCode" column="config_code" type="java.lang.String" length="100" not-null="true"/>
		<property name="configValue" column="config_value" type="java.lang.String" length="200" not-null="true"/>
		<property name="sortNo" column="sort_no"  type="java.lang.Integer"/>
		
		<property name="createDate" type="java.util.Date" column="create_date" />
		<property name="createBy" type="java.lang.String" column="create_by" length="50" />
		<property name="updateDate" type="java.util.Date" column="update_date" />
		<property name="updateBy" type="java.lang.String" column="update_by" length="50" />	
	</class>
</hibernate-mapping>
